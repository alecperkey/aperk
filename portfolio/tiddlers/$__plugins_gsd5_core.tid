        "$:/plugins/gsd5/core/ui/addins/ProjectViewBody": {
            "title": "$:/plugins/gsd5/core/ui/addins/ProjectViewBody",
            "tags": "$:/tags/ViewTemplate",
            "text": "<$list filter=[is[current]field:gsd_type[project]]>\n\n<div class=\"tc-menu-list-item\">\nNext Actions\n<$set name=\"gsd_type\" value=\"action\">\n<$set name=\"gsd_status\" value=\"next\">\n<$set name=\"gsd_complete\" value=\"false\">\n<$set name=\"gsd_project\" value={{!!title}}>\n<$set name=\"gsd_realm\" value={{!!gsd_realm}}>\n<$gsdtiddler skeleton=\"$:/plugins/gsd5/core/ui/skeletons/ActionSkeleton\" title=\"Add new action\" edit={{$:/plugins/gsd5/core/ui/config/new-action-show}} aria-label=\"new action\" class=\"gsd-list-new-button tc-btn-invisible\">\n+\n</$gsdtiddler>\n</$set>\n</$set>\n</$set>\n</$set>\n</$set>\n<hr/>\n<$list filter=[field:gsd_type[action]field:gsd_status[next]field:gsd_complete[false]field:gsd_project{!!title}]>\n<div class=\"tc-menu-list-subitem\">\n<$transclude tiddler=\"$:/plugins/gsd5/core/ui/lists/ListViewPrefix\"/>\n<span class=\"list-link\"><$link to={{!!title}}><$view field=\"title\"/></$link></span>\n<$transclude tiddler=\"$:/plugins/gsd5/core/ui/lists/ListViewSuffix\"/>\n</div>\n</$list>\n</div>\n\n<div class=\"tc-menu-list-item\">\nWaiting Actions\n<$set name=\"gsd_type\" value=\"action\">\n<$set name=\"gsd_status\" value=\"waiting\">\n<$set name=\"gsd_complete\" value=\"false\">\n<$set name=\"gsd_project\" value={{!!title}}>\n<$set name=\"gsd_realm\" value={{!!gsd_realm}}>\n<$gsdtiddler skeleton=\"$:/plugins/gsd5/core/ui/skeletons/ActionSkeleton\" title=\"Add new action\" edit={{$:/plugins/gsd5/core/ui/config/new-action-show}} aria-label=\"new action\" class=\"gsd-list-new-button tc-btn-invisible\">\n+\n</$gsdtiddler>\n</$set>\n</$set>\n</$set>\n</$set>\n</$set>\n<hr/>\n<$list filter=[field:gsd_type[action]field:gsd_status[waiting]field:gsd_complete[false]field:gsd_project{!!title}]>\n<div class=\"tc-menu-list-subitem\">\n<$transclude tiddler=\"$:/plugins/gsd5/core/ui/lists/ListViewPrefix\"/>\n<span class=\"list-link\"><$link to={{!!title}}><$view field=\"title\"/></$link></span>\n<$transclude tiddler=\"$:/plugins/gsd5/core/ui/lists/ListViewSuffix\"/>\n</div>\n</$list>\n</div>\n\n<div class=\"tc-menu-list-item\">\nFuture Actions\n<$set name=\"gsd_type\" value=\"action\">\n<$set name=\"gsd_status\" value=\"future\">\n<$set name=\"gsd_complete\" value=\"false\">\n<$set name=\"gsd_project\" value={{!!title}}>\n<$set name=\"gsd_realm\" value={{!!gsd_realm}}>\n<$gsdtiddler skeleton=\"$:/plugins/gsd5/core/ui/skeletons/ActionSkeleton\" title=\"Add new action\" edit={{$:/plugins/gsd5/core/ui/config/new-action-show}} aria-label=\"new action\" class=\"gsd-list-new-button tc-btn-invisible\">\n+\n</$gsdtiddler>\n</$set>\n</$set>\n</$set>\n</$set>\n</$set>\n<hr/>\n<$list filter=[field:gsd_type[action]field:gsd_status[future]field:gsd_complete[false]field:gsd_project{!!title}]>\n<div class=\"tc-menu-list-subitem\">\n<$transclude tiddler=\"$:/plugins/gsd5/core/ui/lists/ListViewPrefix\"/>\n<span class=\"list-link\"><$link to={{!!title}}><$view field=\"title\"/></$link></span>\n<$transclude tiddler=\"$:/plugins/gsd5/core/ui/lists/ListViewSuffix\"/>\n</div>\n</$list>\n</div>\n\n<div class=\"tc-menu-list-item\">\nSubprojects\n<$set name=\"gsd_type\" value=\"project\">\n<$set name=\"gsd_status\" value=\"active\">\n<$set name=\"gsd_complete\" value=\"false\">\n<$set name=\"gsd_project\" value={{!!title}}>\n<$set name=\"gsd_realm\" value={{!!gsd_realm}}>\n<$gsdtiddler skeleton=\"$:/plugins/gsd5/core/ui/skeletons/ActionSkeleton\" title=\"Add new action\" edit={{$:/plugins/gsd5/core/ui/config/new-project-show}} aria-label=\"new action\" class=\"gsd-list-new-button tc-btn-invisible\">\n+\n</$gsdtiddler>\n</$set>\n</$set>\n</$set>\n</$set>\n</$set>\n<hr/>\n<$list filter=[field:gsd_type[project]field:gsd_project{!!title}]>\n<div class=\"tc-menu-list-subitem\">\n<$transclude tiddler=\"$:/plugins/gsd5/core/ui/lists/ListViewPrefix\"/>\n<span class=\"list-link\"><$link to={{!!title}}><$view field=\"title\"/></$link></span>\n<$transclude tiddler=\"$:/plugins/gsd5/core/ui/lists/ListViewSuffix\"/>\n</div>\n</$list>\n</div>\n\n<div class=\"tc-menu-list-item\">\nReferences\n<$set name=\"gsd_type\" value=\"reference\">\n<$set name=\"gsd_project\" value={{!!title}}>\n<$set name=\"gsd_realm\" value={{!!gsd_realm}}>\n<$gsdtiddler skeleton=\"$:/plugins/gsd5/core/ui/skeletons/ReferenceSkeleton\" title=\"Add new action\" aria-label=\"new action\" class=\"gsd-list-new-button tc-btn-invisible\">\n+\n</$gsdtiddler>\n</$set>\n</$set>\n</$set>\n<hr/>\n<$list filter=[field:gsd_type[reference]field:gsd_project{!!title}]>\n<div class=\"tc-menu-list-subitem\">\n<span class=\"list-link\"><$link to={{!!title}}><$view field=\"title\"/></$link></span>\n</div>\n</$list>\n</div>\n\n<div class=\"tc-menu-list-item\">\nCompleted Actions\n<hr/>\n<$list filter=[field:gsd_type[action]field:gsd_complete[true]field:gsd_project{!!title}!sort[gsd_comp_date]]>\n<div class=\"tc-menu-list-subitem\">\n<$transclude tiddler=\"$:/plugins/gsd5/core/ui/lists/ListViewPrefix\"/>\n<span class=\"list-link\"><$link to={{!!title}}><$view field=\"title\"/></$link></span>\n<$transclude tiddler=\"$:/plugins/gsd5/core/ui/lists/ListViewSuffix\"/>\n</div>\n</$list>\n</div>\n\n</$list>\n"
        },
        "$:/plugins/gsd5/core/ui/addins/ViewBody": {
            "title": "$:/plugins/gsd5/core/ui/addins/ViewBody",
            "text": "\\define dashboard-visibility()\n$:/config/GSD5DashboardCore/visibility/$(listItem)$\n\\end\n\n\\define minimized-notice()\n<$reveal state=<<blueboxState>> type=\"match\" text=\"hide\" default=\"show\">\n<div class=\"gsd-bluebox-min-notice\">//BlueBox minimized//</div>\n</$reveal>\n\\end\n\n<!-- Dashboard -->\n<$list filter=\"[all[shadows+tiddlers]is[current]field:title[Dashboard]]\">\n<$list filter=\"[all[shadows+tiddlers]prefix[$:/config/GSD5DashboardCore/visibility/]!has[draft.of]field:text[show]] [all[shadows+tiddlers]prefix[$:/config/GSD5DashboardPlugin/visibility/]!has[draft.of]field:text[show]] +[limit[1]]\" variable=\"pass\">\n<div class=\"tc-message-box gsd-plugin-box\">\n<<minimized-notice>>\n<$reveal state=<<blueboxState>> type=\"match\" text=\"show\" default=\"show\" retain=\"yes\">\n<div class=\"gsd-plugin-core\">\n<$list filter=\"[all[shadows+tiddlers]tag[$:/tags/GSD5DashboardCore]!has[draft.of]]\" variable=\"listItem\">\n<$reveal state=<<dashboard-visibility>> type=\"match\" text=\"show\" default=\"hide\">\n<$transclude tiddler=<<listItem>>/>\n</$reveal>\n</$list>\n</div>\n<div class=\"gsd-plugin-other\">\n<$list filter=\"[all[shadows+tiddlers]tag[$:/tags/GSD5DashboardPlugin]!has[draft.of]]\" variable=\"listItem\">\n<$transclude tiddler=<<listItem>>/>\n</$list>\n</div>\n</$reveal>\n</div>\n</$list>\n</$list>\n\n<!-- Action -->\n<$list filter=[is[current]field:gsd_type[action]]>\n<$list filter=\"[all[shadows+tiddlers]tag[$:/tags/GSD5ActionCore]!has[draft.of]] [all[shadows+tiddlers]tag[$:/tags/GSD5ActionPlugin]!has[draft.of]] +[limit[1]]\" variable=\"pass\">\n<div class=\"tc-message-box gsd-plugin-box\">\n<<minimized-notice>>\n<$reveal state=<<blueboxState>> type=\"match\" text=\"show\" default=\"show\" retain=\"yes\">\n<div class=\"gsd-plugin-core\">\n<$list filter=\"[all[shadows+tiddlers]tag[$:/tags/GSD5ActionCore]!has[draft.of]]\" variable=\"listItem\">\n<$transclude tiddler=<<listItem>>/>\n</$list>\n</div>\n<div class=\"gsd-plugin-other\">\n<$list filter=\"[all[shadows+tiddlers]tag[$:/tags/GSD5ActionPlugin]!has[draft.of]]\" variable=\"listItem\">\n<$transclude tiddler=<<listItem>>/>\n</$list>\n</div>\n</$reveal>\n</div>\n</$list>\n</$list>\n\n<!-- Project -->\n<$list filter=[is[current]field:gsd_type[project]]>\n<$list filter=\"[all[shadows+tiddlers]tag[$:/tags/GSD5ProjectCore]!has[draft.of]] [all[shadows+tiddlers]tag[$:/tags/GSD5ProjectPlugin]!has[draft.of]] +[limit[1]]\" variable=\"pass\">\n<div class=\"tc-message-box gsd-plugin-box\">\n<<minimized-notice>>\n<$reveal state=<<blueboxState>> type=\"match\" text=\"show\" default=\"show\" retain=\"yes\">\n<div class=\"gsd-plugin-core\">\n<$list filter=\"[all[shadows+tiddlers]tag[$:/tags/GSD5ProjectCore]!has[draft.of]]\" variable=\"listItem\">\n<$transclude tiddler=<<listItem>>/>\n</$list>\n</div>\n<div class=\"gsd-plugin-other\">\n<$list filter=\"[all[shadows+tiddlers]tag[$:/tags/GSD5ProjectPlugin]!has[draft.of]]\" variable=\"listItem\">\n<$transclude tiddler=<<listItem>>/>\n</$list>\n</div>\n</$reveal>\n</div>\n</$list>\n</$list>\n\n<!-- Reference -->\n<$list filter=[is[current]field:gsd_type[reference]]>\n<$list filter=\"[all[shadows+tiddlers]tag[$:/tags/GSD5ReferenceCore]!has[draft.of]] [all[shadows+tiddlers]tag[$:/tags/GSD5ReferencePlugin]!has[draft.of]] +[limit[1]]\" variable=\"pass\">\n<div class=\"tc-message-box gsd-plugin-box\">\n<<minimized-notice>>\n<$reveal state=<<blueboxState>> type=\"match\" text=\"show\" default=\"show\" retain=\"yes\">\n<div class=\"gsd-plugin-core\">\n<$list filter=\"[all[shadows+tiddlers]tag[$:/tags/GSD5ReferenceCore]!has[draft.of]]\" variable=\"listItem\">\n<$transclude tiddler=<<listItem>>/>\n</$list>\n</div>\n<div class=\"gsd-plugin-other\">\n<$list filter=\"[all[shadows+tiddlers]tag[$:/tags/GSD5ReferencePlugin]!has[draft.of]]\" variable=\"listItem\">\n<$transclude tiddler=<<listItem>>/>\n</$list>\n</div>\n</$reveal>\n</div>\n</$list>\n</$list>\n\n<!-- Contact -->\n<$list filter=[is[current]field:gsd_type[contact]]>\n<$list filter=\"[all[shadows+tiddlers]tag[$:/tags/GSD5ContactCore]!has[draft.of]] [all[shadows+tiddlers]tag[$:/tags/GSD5ContactPlugin]!has[draft.of]] +[limit[1]]\" variable=\"pass\">\n<div class=\"tc-message-box gsd-plugin-box\">\n<<minimized-notice>>\n<$reveal state=<<blueboxState>> type=\"match\" text=\"show\" default=\"show\" retain=\"yes\">\n<div class=\"gsd-plugin-core\">\n<$list filter=\"[all[shadows+tiddlers]tag[$:/tags/GSD5ContactCore]!has[draft.of]]\" variable=\"listItem\">\n<$transclude tiddler=<<listItem>>/>\n</$list>\n</div>\n<div class=\"gsd-plugin-other\">\n<$list filter=\"[all[shadows+tiddlers]tag[$:/tags/GSD5ContactPlugin]!has[draft.of]]\" variable=\"listItem\">\n<$transclude tiddler=<<listItem>>/>\n</$list>\n</div>\n</$reveal>\n</div>\n</$list>\n{{||$:/plugins/gsd5/core/ui/lists/contacts-action-list}}\n</$list>\n\n<!-- Context -->\n<$list filter=[is[current]field:gsd_type[context]]>\n<$list filter=\"[all[shadows+tiddlers]tag[$:/tags/GSD5ContextCore]!has[draft.of]] [all[shadows+tiddlers]tag[$:/tags/GSD5ContextPlugin]!has[draft.of]] +[limit[1]]\" variable=\"pass\">\n<div class=\"tc-message-box gsd-plugin-box\">\n<<minimized-notice>>\n<$reveal state=<<blueboxState>> type=\"match\" text=\"show\" default=\"show\" retain=\"yes\">\n<div class=\"gsd-plugin-core\">\n<$list filter=\"[all[shadows+tiddlers]tag[$:/tags/GSD5ContextCore]!has[draft.of]]\" variable=\"listItem\">\n<$transclude tiddler=<<listItem>>/>\n</$list>\n</div>\n<div class=\"gsd-plugin-other\">\n<$list filter=\"[all[shadows+tiddlers]tag[$:/tags/GSD5ContextPlugin]!has[draft.of]]\" variable=\"listItem\">\n<$transclude tiddler=<<listItem>>/>\n</$list>\n</div>\n</$reveal>\n</div>\n</$list>\n</$list>\n\n<!-- Reference Dashboard -->\n<$list filter=\"[all[shadows+tiddlers]is[current]field:title[References]]\">\n<div class=\"tc-message-box\">\n<<minimized-notice>>\n<$reveal state=<<blueboxState>> type=\"match\" text=\"show\" default=\"show\" retain=\"yes\">\n\n<$checkbox tiddler=\"$:/config/GSD5ReferencesGroup\" field=\"text\" checked=\"gsd_project\" unchecked=\"none\">&nbsp;Group By Project</$checkbox>\n\n</$reveal>\n</div>\n</$list>\n"
        },
        "$:/plugins/gsd5/core/ui/addins/ViewComplete": {
            "title": "$:/plugins/gsd5/core/ui/addins/ViewComplete",
            "text": "<$list filter=[is[current]field:gsd_type[action]][is[current]field:gsd_type[project]]>\n<$complete></$complete>\n</$list>\n"
        },
        "$:/plugins/gsd5/core/ui/addins/ViewContact": {
            "title": "$:/plugins/gsd5/core/ui/addins/ViewContact",
            "tags": "$:/tags/GSD5ActionCore $:/tags/GSD5ProjectCore",
            "text": "<p class=\"gsd-bluebox-droplist\">\n    Contact:<br/>\n    <$select field=\"gsd_contact\" default=\"none\">\n        <option value=\"\">--</option>\n        <$set name=\"parentTiddler\" value=<<currentTiddler>> >\n            <$list filter=\"[is[current]!field:gsd_realm[all]]\" >\n                <optgroup label={{!!gsd_realm}}>\n                    <$list filter=[field:gsd_type[contact]field:gsd_realm{!!gsd_realm}]>\n                        <option value=<<currentTiddler>>><<currentTiddler>></option>\n                    </$list>\n                </optgroup>\n            </$list>\n            <optgroup label=\"All Realms\">\n                <$list filter=[field:gsd_type[contact]field:gsd_realm[all]]>\n                    <option value=<<currentTiddler>>><<currentTiddler>></option>\n                </$list>\n            </optgroup>\n        </$set>\n    </$select>\n    <span class=\"goto-link\">\n        <$list filter=[is[current]!field:gsd_contact[]]>\n            &nbsp;<$link to={{!!gsd_contact}}>>></$link>\n        </$list>\n        <$list filter=[is[current]field:gsd_contact[]]>\n        <$set name=\"gsd_type\" value=\"contact\">\n        <$set name=\"gsd_realm\" value={{$:/currentRealm}}>\n        &nbsp;<$gsdtiddler class=\"tc-btn-invisible\" setNow=\"true\" skeleton=\"$:/plugins/gsd5/core/ui/skeletons/ContactSkeleton\" edit={{$:/plugins/gsd5/core/ui/config/new-tiddler-show}}><span style=\"text-decoration:underline\">new</span></$gsdtiddler>\n        </$set>\n        </$set>\n        </$list>\n    </span>\n</p>\n"
        },
        "$:/plugins/gsd5/core/ui/addins/ViewContext": {
            "title": "$:/plugins/gsd5/core/ui/addins/ViewContext",
            "tags": "$:/tags/GSD5ActionPlugin",
            "text": "\\define unique-id()\n$(currentTiddler)$-toggle\n\\end\n\n<p>\n    <div class=\"gsd-context-box\">\n        <$set name=\"parentTiddler\" value=<<currentTiddler>>>\n        <$set name=\"ContextStatus\" value=<<qualify \"$:/state/popup/context\">>>\n            <$button popup=<<ContextStatus>> class=\"tc-btn-invisible gsd-context-button\">+ Contexts</$button>\n            <$reveal state=<<ContextStatus>> type=\"nomatch\" text=\"\" class=\"tc-popup-handle\" default=\"\" retain=\"yes\" animate=\"yes\">\n            <div class=\"tc-menu-list-subitem\">\n\n\t\t\t<$list filter=\"[field:gsd_type[context]field:gsd_realm{!!gsd_realm}] [field:gsd_type[context]field:gsd_realm[all]]\" emptyMessage=\"<em>-no contexts-</em>\">\n\t\t\t\t<div class=\"tc-menu-list-subitem\">\n\t\t\t\t<$checkbox tiddler=<<parentTiddler>> tag=<<currentTiddler>>>\n\t\t\t\t\t<<currentTiddler>>\n\t\t\t\t</$checkbox>\n\t\t\t\t</div>\n\t\t\t</$list>\n\n\t\t\t</div>\n\t\t\t</$reveal>\n\t\t</$set>\n        </$set>\n    </div>\n</p>"
        },
        "$:/plugins/gsd5/core/ui/addins/ViewPrereq": {
            "title": "$:/plugins/gsd5/core/ui/addins/ViewPrereq",
            "tags": "$:/tags/GSD5ActionCore $:/tags/GSD5ProjectCore $:/tags/GSD5ReferenceCore",
            "text": "\\define gsd-tiddler-fly(type,status)\n<$set name=\"gsd_type\" value=$type$>\n<$set name=\"gsd_status\" value=$status$>\n<$set name=\"gsd_complete\" value=\"false\">\n<$set name=\"gsd_realm\" value={{$:/currentRealm}}>\n<$gsdtiddler class=\"tc-btn-invisible\" setNow=\"true\" skeleton=\"$:/plugins/gsd5/core/ui/skeletons/ActionSkeleton\" edit={{$:/plugins/gsd5/core/ui/config/new-tiddler-show}}><span style=\"text-decoration:underline\">new</span></$gsdtiddler>\n</$set>\n</$set>\n</$set>\n</$set>\n\\end\n\n<$list filter=[is[current]field:gsd_type[project]]>\n<p class=\"gsd-bluebox-droplist\">\nSubproject of: <br/>\n<$select field=\"gsd_project\" default=\"\">\n  <option value=\"\">--</option>\n  <$list filter=\"[field:gsd_type[project]!field:gsd_complete[true]!is[current]field:gsd_realm{!!gsd_realm}]\">\n  <option value=<<currentTiddler>>><<currentTiddler>></option>\n  </$list>\n  </$select>\n  <span class=\"goto-link\">\n  <$list filter=[is[current]!field:gsd_project[]]>\n  &nbsp;<$link to={{!!gsd_project}}>>></$link>\n  </$list>\n  <$list filter=\"[is[current]field:gsd_project[]]\">\n  &nbsp;<<gsd-tiddler-fly \"project\" \"active\">>\n  </$list>\n  </span>\n</p>\n</$list>\n\n<$list filter=[is[current]field:gsd_type[action]]>\n<p class=\"gsd-bluebox-droplist\">\nProject of: <br/>\n<$select field=\"gsd_project\" default=\"\">\n  <option value=\"\">--</option>\n  <$list filter=\"[field:gsd_type[project]!field:gsd_complete[true]field:gsd_realm{!!gsd_realm}]\">\n  <option value=<<currentTiddler>>><<currentTiddler>></option>\n  </$list>\n</$select>\n<span class=\"goto-link\">\n  <$list filter=[is[current]!field:gsd_project[]]>\n    &nbsp;<$link to={{!!gsd_project}}>>></$link>\n  </$list>\n  <$list filter=\"[is[current]field:gsd_project[]]\">\n    &nbsp;<<gsd-tiddler-fly \"project\" \"active\">>\n  </$list>\n</span>\n</p>\n<p class=\"gsd-bluebox-droplist\">\nDepends on: <br/>\n<$prereq default=\"\">\n  <option value=\"\">--</option>\n  <$set name=\"currentTitle\" value=<<currentTiddler>> >\n    <$list filter=\"[field:gsd_type[action]!field:gsd_complete[true]field:gsd_realm{$:/currentRealm!!text}!has[gsd_project]limit[1]]\">\n      <optgroup label=\"No Project\">\n        <$list filter=\"[field:gsd_type[action]!field:gsd_complete[true]field:gsd_realm{$:/currentRealm!!text}!has[gsd_project]!title<currentTitle>]\">\n          <option value=<<currentTiddler>>><<currentTiddler>></option>\n        </$list>\n      </optgroup>\n    </$list>\n  </$set>\n<$set name=\"currentTitle\" value=<<currentTiddler>> >\n  <$list filter=\"[field:gsd_type[action]!field:gsd_complete[true]field:gsd_realm{$:/currentRealm!!text}has[gsd_project]each[gsd_project]]\">\n    <optgroup label={{!!gsd_project}}>\n     <$list filter=\"[field:gsd_type[action]!field:gsd_complete[true]!title<currentTitle>field:gsd_project{!!gsd_project}]\">\n       <option value=<<currentTiddler>>><<currentTiddler>></option>\n     </$list>\n    </optgroup>\n  </$list>\n</$set>\n</$prereq>\n<span class=\"goto-link\">\n  <$list filter=[is[current]!field:gsd_action[]]>\n    &nbsp;<$link to={{!!gsd_action}}>>></$link>\n  </$list>\n  <$list filter=\"[is[current]field:gsd_action[]]\">\n    &nbsp;<<gsd-tiddler-fly \"action\" \"next\">>\n  </$list>\n</span>\n</p>\n</$list>\n\n<$list filter=[is[current]field:gsd_type[reference]]>\n<p class=\"gsd-bluebox-droplist\">\nProject of: <br/>\n<$select field=\"gsd_project\" default=\"\">\n  <option value=\"\">--</option>\n  <$list filter=\"[field:gsd_type[project]!field:gsd_complete[true]field:gsd_realm{!!gsd_realm}]\">\n  <option value=<<currentTiddler>>><<currentTiddler>></option>\n  </$list>\n  </$select>\n  <span class=\"goto-link\">\n  <$list filter=[is[current]!field:gsd_project[]]>\n  &nbsp;<$link to={{!!gsd_project}}>>></$link>\n  </$list>\n  <$list filter=\"[is[current]field:gsd_project[]]\">\n  &nbsp;<<gsd-tiddler-fly \"project\" \"active\">>\n  </$list>\n  </span>\n</p>\n</$list>\n"
        },
        "$:/plugins/gsd5/core/ui/addins/ViewRealm": {
            "title": "$:/plugins/gsd5/core/ui/addins/ViewRealm",
            "tags": "$:/tags/GSD5ActionCore $:/tags/GSD5ProjectCore $:/tags/GSD5ReferenceCore $:/tags/GSD5ContactCore $:/tags/GSD5ContextCore",
            "text": "<p class=\"gsd-bluebox-droplist\">\nRealm:<br/>\n<$select field=\"gsd_realm\" default=\"\">\n<option value=\"all\">--</option>\n<$list filter=\"[field:gsd_type[realm]]\">\n<option value=<<currentTiddler>>><<currentTiddler>></option>\n</$list>\n</$select>\n</p>\n"
        },
        "$:/plugins/gsd5/core/ui/addins/ViewStatus": {
            "title": "$:/plugins/gsd5/core/ui/addins/ViewStatus",
            "tags": "$:/tags/GSD5ActionCore $:/tags/GSD5ProjectCore",
            "text": "<$list filter=[is[current]field:gsd_type[action]!field:gsd_complete[true]]>\n<p class=\"gsd-bluebox-droplist\">\n<$radio class=\"body-status next\" field=\"gsd_status\" value=\"next\">next</$radio>\n<$radio class=\"body-status waiting\" field=\"gsd_status\" value=\"waiting\">waiting</$radio>\n<$radio class=\"body-status future\" field=\"gsd_status\" value=\"future\">future</$radio>\n</p>\n</$list>\n\n<$list filter=[is[current]field:gsd_type[project]!field:gsd_complete[true]]>\n<p class=\"gsd-bluebox-droplist\">\n<$radio class=\"body-status next\" field=\"gsd_status\" value=\"active\">active</$radio>\n<$radio class=\"body-status future\" field=\"gsd_status\" value=\"future\">future</$radio>\n</p>\n</$list>\n"
        }
    }
}