        "$:/plugins/gsd5/core/ui/stylesheet/gsd5-stylesheet": {
            "title": "$:/plugins/gsd5/core/ui/stylesheet/gsd5-stylesheet",
            "tags": "$:/tags/Stylesheet",
            "text": ".tc-complete-box {\n  position: relative;\n  vertical-align: middle;\n}\n\n.body-status input[type=\"radio\"] {\n  display: none;\n}\n\n.body-status span {\n  display:inline-block;\n  padding:0.1em 0.8em;\n  font-weight: bold;\n  color: silver;\n  border: #EFD982 1px solid;\n}\n\n.list-status input[type=\"radio\"] {\n  display: none;\n}\n\n.list-status span {\n  font-size: small;\n  margin-top: 0.3em;\n  padding: 0em 0.3em;\n  display:inline-block;\n  color: silver;\n  border: #EFD982 1px solid;\n}\n\n.list-link {\n  margin-left: 0.5em;\n}\n\n.next input[type=\"radio\"]:checked + span {\n  background-color: #BCE8C3;\n  color: darkgreen;\n  border: darkgreen 1px solid;\n}\n\n.waiting input[type=\"radio\"]:checked + span {\n  background-color: #FFDEAD;\n  color: darkgoldenrod;\n  border: darkgoldenrod 1px solid;\n}\n\n.future input[type=\"radio\"]:checked + span {\n  background-color: #A8DCF6;\n  color: #1C88BD;\n  border: #1C88BD 1px solid;\n}\n\n.content-box:after {\n  content: \"\";\n  display: table;\n  clear: both;\n}\n\n.content-left {\n  //background-color: lightgreen;\n  width: 50%;\n  float: left;\n}\n\n.gsd5-project-list {\n  padding-top: 5px;\n}\n\n.gsd5-project-list:first-of-type {\n  padding-top: 0;\n}\n\n.gsd-plugin-box:after {\n  content: \"\";\n  display: table;\n  clear: both;\n}\n\n@media (min-width: {{$:/themes/tiddlywiki/vanilla/metrics/sidebarbreakpoint}}) {\n  .gsd-plugin-core {\n    padding-top: 0.5em;\n    padding-bottom: 0.5em;\n    max-width: 50%;\n    min-width: 50%;\n    float: left;\n  }\n\n  .gsd-plugin-other {\n    padding-top: 0.5em;\n    padding-bottom: 0.5em;\n    max-width: 50%;\n    min-width: 50%;\n    float: right;\n  }\n}\n\n@media (max-width: {{$:/themes/tiddlywiki/vanilla/metrics/sidebarbreakpoint}}) {\n  .gsd-plugin-core {\n    padding-top: 0.5em;\n  }\n\n  .gsd-plugin-other {\n    padding-bottom: 0.5em;\n  }\n}\n\n.gsd-context-box {\n  border: #dce0ea 1px solid;\n  background-color: #eff4ff;\n  padding-left: 5px;\n}\n\n.gsd-context-button {\n  width:100%;\n  text-align:left;\n}\n\n.gsd-list-new-button {\n  font-size:1.5em;\n}\n\n.gsd-bluebox-title{\n  font-size:1.33em;\n  width:100%;\n}\n\n.gsd-bluebox-toggle{\n  display:inline;\n  vertical-align:bottom;\n  fill:<<colour tiddler-controls-foreground>>;\n}\n\n.gsd-bluebox-toggle:hover{\n  fill:<<colour tiddler-controls-foreground-hover>>;\n}\n\n.gsd-bluebox-toggle input{\n  display:none;\n}\n\n.gsd-bluebox-toggle svg {\n  vertical-align:middle;\n  height:0.75em;\n}\n\n.gsd-bluebox-min-notice {\n  width:100%;\n}\n\n.gsd-muted {\n  color: <<colour muted-foreground>>;\n}\n\n.gsd-muted svg {\n  opacity: 0.25;\n}\n\n.gsd-list-title input[type=checkbox]{\n  display:none;\n}\n\n.gsd-list-title input[type=checkbox] + span svg {\n  fill: <<colour tiddler-controls-foreground>>;\n  height: 0.8em;\n}"
        },
